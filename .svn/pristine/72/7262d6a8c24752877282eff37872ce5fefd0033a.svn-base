body,
html {
  font-family: 'Microsoft Yahei';
  background: white;
  color: #2a383f;
}
i,
em,
strong {
  font-style: normal;
}
.Detail {
  /* margin-top: 100px; */
  width: 100%;
  /*padding-left: 320px;*/
  /*padding-right: 20px;*/
  font-size: 14px;
}
.Detail_cont {
  display: none;
  /*margin-top: 16px;*/
  width: 100%;
  height: calc(100% - 30px);
  /*border: 1px solid #eee;*/
  /*box-shadow: 0px 3px 3px #eee;*/
}
.Detail_cont .file_lose {
  display: block;
  position: relative;
  width: 100%;
  height: 500px;
  padding-top: 104px;
  text-align: center;
}
.Detail_cont .file_lose em {
  display: block;
  margin-top: 20px;
  margin-bottom: 22px;
  color: #333;
  font-size: 14px;
}
.Detail_cont .file_lose span {
  font-size: 12px;
  color: #808080;
}
.Detail_cont .file_lose button {
  margin-top: 50px;
  width: 90px;
  height: 34px;
  line-height: 34px;
  border-radius: 4px;
  color: white;
  border: none;
  background: #157cf8;
}
.Detail_cont .file_lose button:active {
  background: #1973e0;
}
.Detail_cont .file_lose .pup {
  position: absolute;
  width: 480px;
  height: 78px;
  top: 167px;
  left: 50%;
  -webkit-transform: translate(-50%, 0%);
  -moz-transform: translate(-50%, 0%);
  -ms-transform: translate(-50%, -50%);
  -o-transform: translate(-50%, 0%);
  background: rgba(0, 0, 0, 0.55);
  border-radius: 4px;
  text-align: center;
  color: white;
  font-size: 12px;
  letter-spacing: 2px;
  display: none;
}
.Detail_cont .file_lose .pup p:nth-child(1) {
  margin-top: 21px;
}
.Detail_cont .no_html {
  width: 100%;
  /*margin-top: 30px;*/
  height: 606px;
  overflow-y: auto;
  display: block;
}
.Detail .Detail_main {
  /* margin-top: 16px; */
  /*width: max-content;*/
}
@media screen and (max-width: 414px) and (min-width: 320px){
  .Detail .Detail_main {
    width: max-content;
  }
}


.Detail .Detail_main .Detail_left {
  position: absolute;
  top: 16px;
  left: 0px;
  width: 306px;
  border: 1px solid #e1e9ed;
  box-shadow: 0px 3px 3px #eee;
}
.Detail .Detail_main .Detail_left .Detail_left_title {
  width: 100%;
  height: 48px;
  line-height: 48px;
  background: #f0f0f0;
}
.Detail .Detail_main .Detail_left .Detail_left_title li {
  float: left;
  width: 152px;
  text-align: center;
  color: #333333;
  font-weight: bold;
  border-bottom: 1px solid #daecff;
  cursor: pointer;
}
.Detail .Detail_main .Detail_left .Detail_left_title .liAct {
  border-left: 1px solid #daecff;
  border-right: 1px solid #daecff;
  border-bottom: none !important;
  border-top: 2px solid #3c7eec;
  background: white;
  color: #3c7eec;
  font-weight: 700;
}
.Detail .Detail_main .Detail_left .main {
  display: none;
  height: 630px;
  overflow-y: auto;
}
.Detail .Detail_main .Detail_left .main1 {
  width: 100%;
  display: block;
}
.Detail .Detail_main .Detail_left .main1 .no_main1 {
  margin-top: 142px;
  text-align: center;
  font-size: 14px;
  color: #999;
  display: none;
}
.Detail .Detail_main .Detail_left .main1 .main1_list {
  width: 100%;
  font-size: 14px;
  color: #333;
/*   border-bottom: 1px solid #efefef; */
}
.Detail .Detail_main .Detail_left .main1 .main1_list dt {
  width: 100%;
  padding: 0px 10px 12px 10px;
  cursor: pointer;
}
.Detail .Detail_main .Detail_left .main1 .main1_list dt img {
  margin-right: 7px;
}
.Detail .Detail_main .Detail_left .main1 .main1_list dt span {
  display: inline-block;
  max-width: 230px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  vertical-align: middle;
}
.Detail .Detail_main .Detail_left .main1 .main1_list dt em {
  color: #f4402c;
  vertical-align: middle;
}
