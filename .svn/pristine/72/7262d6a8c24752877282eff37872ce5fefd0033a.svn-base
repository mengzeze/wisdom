  transform: rotate(180deg);
}
.Detail .Detail_main .Detail_left .main1 .main1_list dd {
  width: 100%;
  padding-left: 42px;
  padding-bottom: 12px;
  padding-right: 12px;
  font-size: 12px;
}
.Detail .Detail_main .Detail_left .main1 .main1_list dd .two_tit span {
  display: inline-block;
  max-width: 215px;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  vertical-align: middle;
}
.Detail .Detail_main .Detail_left .main1 .main1_list dd em {
  color: #f4402c;
  vertical-align: middle;
}
.Detail .Detail_main .Detail_left .main1 .main1_list dd .main1_key {
  display: none;
  margin-top: 12px;
  /* margin-bottom: 12px; */
  width: 240px;
  border: 1px solid #efefef;
  border-bottom: none;
  background: #fbfbfd;
}
.Detail .Detail_main .Detail_left .main1 .main1_list dd .main1_key li {
  border-bottom: 1px solid #efefef;
  padding: 10px 26px;
  color: #f4402c;
}
.Detail .Detail_main .Detail_left .main1 .main1_list dd .main1_key li span {
  margin-right: 10px;
}
.Detail .Detail_main .Detail_left .main2 {
